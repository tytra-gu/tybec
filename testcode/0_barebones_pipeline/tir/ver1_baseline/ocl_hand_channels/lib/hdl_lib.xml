      <AVALON port="oready" type="oready"/>
      <INPUT  port="ka_vin0" width="32"/>
      <INPUT  port="ka_vin1" width="32"/>
      <OUTPUT port="ka_vout" width="32"/>
    </INTERFACE>
    <C_MODEL>
      <FILE name="c_model.cl" />
    </C_MODEL>
    <REQUIREMENTS>
      <FILE name="hdl/func_hdl_top_kernel_A.v" />
      <FILE name="hdl/kernelTop_kernel_A.v" />
      <FILE name="hdl/kernel_A_local1.v" />
